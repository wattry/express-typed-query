{
  "name": "express-typed-query",
  "version": "1.0.26",
  "description": "Converts query strings(qs) to Javascript typed objects including dates and malformed JSON",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/es/index.js",
  "files": [
    "./dist",
    "!./dist/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "concurrently 'pnpm:build:cjs' 'pnpm:build:es' 'pnpm:build:types'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:types": "tsc -p tsconfig.types.json",
    "serve:global": "nodemon --exec ts-node examples/global.ts",
    "serve:routes": "nodemon --exec ts-node examples/routes.ts",
    "ci": "concurrently 'pnpm test'",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "query-string",
    "qs",
    "express",
    "typescript",
    "types"
  ],
  "repository": "https://github.com/wattry/express-typed-query",
  "author": "wattry",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.6",
    "@types/qs": "^6.9.15",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "eslint-plugin-jest": "^28.6.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  },
  "dependencies": {
    "qs": "^6.12.1",
    "tslib": "^2.6.3"
  }
}
